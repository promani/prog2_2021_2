<%- include('./partials/head') %> 
<!-- Begin page content -->
<main class="feed">
  <form action='/search'>
    <div class="row">
      <div class="col-10">
        <div class="mb-3">
          <input type="text" class="form-control" name="criteria" id="criteria" placeholder="Buscar" value="<%= criteria %>">
        </div>
      </div>
      <div class="col-2">
        <button type="submit" class="btn btn-primary">Submit</button>
      </div>
    </div>
  </form>
  <div class="alert alert-danger" role="alert">
    Lo sentimos, no hemos encontrado resultados para <%= criteria %> .
  </div>
</main>
<%- include('./partials/footer') %> 

